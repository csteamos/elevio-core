{
  "name": "elevio-core",
  "version": "0.0.1",
  "description": "The core implementation of Elevio. A system that tracks, analyzes and reports weak points for students in real-time.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "import": "./dist/models/index.js",
      "require": "./dist/models/index.js"
    },
    "./models/topic": {
      "types": "./dist/models/topic/index.d.ts",
      "import": "./dist/models/topic/index.js",
      "require": "./dist/models/topic/index.js"
    },
    "./models/weakpoint": {
      "types": "./dist/models/weakpoint/index.d.ts",
      "import": "./dist/models/weakpoint/index.js",
      "require": "./dist/models/weakpoint/index.js"
    },
    "./integrations": {
      "types": "./dist/integrations/index.d.ts",
      "import": "./dist/integrations/index.js",
      "require": "./dist/integrations/index.js"
    },
    "./scoring": {
      "types": "./dist/scoring/index.d.ts",
      "import": "./dist/scoring/index.js",
      "require": "./dist/scoring/index.js"
    },
    "./weakpoints": {
      "types": "./dist/weakpoints/index.d.ts",
      "import": "./dist/weakpoints/index.js",
      "require": "./dist/weakpoints/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csteamos/elevio-core.git"
  },
  "keywords": [
    "elevio",
    "weak-point",
    "detection",
    "real-time",
    "core"
  ],
  "author": "Ismail El Badawy (ismailelbadawyw@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/csteamos/elevio-core/issues"
  },
  "homepage": "https://github.com/csteamos/elevio-core#readme",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
